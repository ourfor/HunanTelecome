        <item name="android:button">@drawable/cb_login_selector</item>
    </style>
    <style name="login_gray">
        <item name="android:textColor">#ffababab</item>
    </style>
    <style name="login_green">
        <item name="android:textColor">#ff25a52c</item>
    </style>
    <style name="login_out_color">
        <item name="android:textColor">#ffcd8323</item>
    </style>
